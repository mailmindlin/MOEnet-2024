{
	"$schema": "./schema.json",
	"timer": "system",
	"log": {
		"level": "DEBUG"
	},
	"nt": {
		"team": 365,
		"host": "localhost",
		"port": 5810,
		"publishErrors": true,
		"publishPing": true,
		"subscribeSleep": false,
