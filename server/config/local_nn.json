{
	"$schema": "./schema.json",
	"timer": "system",
	"log": {
		"level": "DEBUG"
	},
	"datalog": {
		"enabled": false,
		"folder": "../../log"
	},
	"nt": {
		"team": 365,
		"publishErrors": true,
		"publishPing": true,
		"subscribeSleep": false,
		"subscribeConfig": false,
		"publishDetections": true,
		"transforms": {
			"fieldToOdom": "sub",
			"fieldToRobot": "pub",
			"odomToRobot": "pub"
		},
		"publishField2dF2O": false,
		"publishField2dF2R": false,
		"publishField2dDets": true
	},
	"pipelines": [
		{
			"id": "nn:moenet-2023-v1",
			"stages": [
				{
					"stage": "rgb",
					"resolution": "THE_1080_P"
				},
				{
					"stage": "depth"
				},
				{
					"stage": "nn",
					"blobPath": "./moenet-2023-v1/nn.blob",
					"config": "./moenet-2023-v1/pipeline.json"
				}
			]
		},
		{
			"id": "nn:moenet-2024-v1",
			"stages": [
				{
					"stage": "rgb",
					"resolution": "THE_1080_P"
				},
				{
					"stage": "depth"
				},
				{
					"stage": "nn",
					"blobPath": "./moenet-2024-v1/nn.blob",
					"config": "./moenet-2024-v1/pipeline.json"
				}
			]
		}
	],
	"camera_selectors": [
		{
			"id": "main",
			"ordinal": 1
		}
	],
	"cameras": [
		{
			"id": "S2",
			"selector": "main",
			"pose": {
				"translation": {
					"x": 0,
					"y": 0,
					"z": 0
				},
				"rotation": {
					"quaternion": {
						"W": 1,
						"X": 0,
						"Y": 0,
						"Z": 0
					}
				}
			},
			"pipeline": [
				{
					"stage": "inherit",
					"id": "nn:moenet-2024-v1"
				}
			],
			"slam": false
		}
	]
}